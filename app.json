{
  "name": "QUEEN-SENU-MD",
  "description": "QUEEN SENU is simple multidevice whatsapp bot deverloped by CyberKillers Team",
  "logo": "https://i.ibb.co/kyqXtXm/1708757084147.jpg",
  "keywords": ["senu", "bot", "whatsapp bot", "multi device", "queen-senu-md"],
  "success_url": "/",

  "env": {
    "BOT_NUMBER": {
      "description": "Put the number here.",
      "required": true
    },  

    "PASSWORD": {
      "description": "Enter a password of your choice here",
      "required": true
    },

    "SESSION_ID": {
      "description": "Enter session-id here",
      "required": false
    },
    
    "GITHUB_USERNAME": {
      "description": "Put your github username here.",
      "required": true
    },
    "GITHUB_AUTH_TOKEN": {
      "description": "Put your github auth-token here.",
      "required": true
    }   
},

     "buildpacks": [
        {
            "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
        }
     ],
  "stack": "heroku-22"
}
